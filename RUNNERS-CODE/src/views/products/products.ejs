<!DOCTYPE html>
<html>

<%- include('../partials/head') %>

<body>
  <%- include('../partials/header') %>
  <main>
    <!-- todos los productos -->
    <div class="container products-wrapper">
      <div class="row">
        <div class="col-12">
          <h2 class="products-title">Todos los productos</h2>
        </div>
        <!-- Categoria Mujer -->
        <div class="col-12">
          <h2 class="category-title">Mujer</h2>
        </div>
        <div >
        <% for( let i = 0; i < products.length; i++ ) { %>
          <li>
              <a  href="/products/<%= products[i].id %>"><%= products[i].name %></a>
          </li>
          <% } %>
        </div>
  </main>
  <%- include('../partials/footer') %>
</body>

</html>